{
  "name": "github-reserved-names",
  "version": "1.1.0",
  "description": "Get a list, or check if a user or organization name is reserved by GitHub",
  "license": "MIT",
  "repository": "Mottie/github-reserved-names",
  "author": {
    "name": "Rob Garrison",
    "email": "wowmotty@gmail.com"
  },
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "github-reserved": "cli.js"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js",
    "cli.js",
    "reserved-names.json",
    "oddballs.json"
  ],
  "keywords": [
    "github",
    "reserved",
    "username",
    "organization",
    "cli"
  ],
  "xo": {
    "space": false,
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "quote-props": 0
    }
  },
  "dependencies": {
    "meow": "^5.0.0"
  },
  "devDependencies": {
    "ava": "*",
    "xo": "^0.21.0"
  }
}
