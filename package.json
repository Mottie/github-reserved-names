{
  "name": "github-reserved-names",
  "version": "2.0.0",
  "description": "Get a list, or check if a user or organization name is reserved by GitHub",
  "license": "MIT",
  "repository": "Mottie/github-reserved-names",
  "author": {
    "name": "Rob Garrison",
    "email": "wowmotty@gmail.com"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "reserved-names.json",
    "oddballs.json"
  ],
  "types": "./index.d.ts",
  "keywords": [
    "github",
    "reserved",
    "username",
    "organization",
    "cli"
  ],
  "xo": {
    "space": false,
    "rules": {
      "quotes": [
        "error",
        "double"
      ],
      "quote-props": 0
    }
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "xo": "^0.25.3"
  }
