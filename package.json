{
  "name": "github-reserved-names",
  "version": "2.1.0",
  "description": "Get a list, or check if a user or organization name is reserved by GitHub",
  "license": "MIT",
  "repository": "Mottie/github-reserved-names",
  "author": {
    "name": "Rob Garrison",
    "email": "wowmotty@gmail.com"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "reserved-names.json",
    "oddballs.json"
  ],
  "types": "./index.d.ts",
  "keywords": [
    "github",
    "reserved",
    "username",
    "organization"
  ],
  "xo": {
    "space": false,
    "rules": {
      "unicorn/prefer-module": 0,
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "devDependencies": {
    "ava": "^6.1.3",
    "xo": "^0.59.3"
  }
}
